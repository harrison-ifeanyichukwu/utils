{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "single",
            "jsx-single"
        ],
        "no-implicit-dependencies": false,
        "no-unused-expression": false,
        "curly": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-access": false
    },
    "rulesDirectory": []
}