{
    "compilerOptions": {
        "declaration": true,
        "emitDeclarationOnly": true,
        "declarationDir": "./typings",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "outDir": "./lib",
        "sourceMap": true,
        "downlevelIteration": true,
        "lib": [
            "dom",
            "es5",
            "es2015.collection",
            "es2015.iterable"
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": ["node_modules"]
}